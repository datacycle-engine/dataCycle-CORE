<% map_filter_layers, filter_bbox = map_filter_layers(@filters) %>

<div class="geographic">
  <div id="map_<%= SecureRandom.hex(16) %>" class="geographic-map dashboard"
          data-map-options="<%= DataCycleCore.default_map_position.presence&.to_json %>"
          data-map-bounds="<%= filter_bbox || contents.to_bbox %>"
          data-language="<%= @language %>"
          data-filter-layers="<%= map_filter_layers.presence&.to_json %>"
          >
  </div>
</div>
