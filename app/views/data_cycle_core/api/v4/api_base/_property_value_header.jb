# frozen_string_literal: true

json ||= {}
key_name = attribute_key(key, definition)
api_property_definition = api_definition(definition)

json['@type'] = api_property_definition['type'] if api_property_definition['type'].present?
json['@type'] = api_property_definition.dig('transformation', 'type') if api_property_definition.dig('transformation', 'type').present?
json['identifier'] = key_name.camelize(:lower)
json['name'] = definition['label']
json['unitCode'] = api_property_definition['unit_code'] if api_property_definition['unit_code'].present?
json['unitText'] = api_property_definition['unit_text'] if api_property_definition['unit_text'].present?

json
