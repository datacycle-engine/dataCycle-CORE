<% I18n.with_locale(local_assigns[:locale] || I18n.locale) do %>
  <% @results.each do |object| %>
    <%= render_linked_partial(
      key: local_assigns[:key] + (local_assigns[:definition]['type'] == "object" ? '[][id]' : '[]'),
      definition: local_assigns[:definition],
      parameters: {
        object: object,
        prefix: local_assigns[:prefix],
        options: local_assigns[:options],
        objects: local_assigns[:objects],
        editable: local_assigns[:editable],
        edit_buttons: true,
        parent: @parent
      },
      content: @content
    ) %>
  <% end %>
<% end %>
