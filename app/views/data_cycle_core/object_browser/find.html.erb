<% @objects.each do |object| %>
  <%= render_linked_partial(
    key: params[:key] + (params[:definition]['type'] == "object" ? '[][id]' : '[]'),
    definition: params[:definition],
    parameters: {
      object: object,
      prefix: params[:prefix],
      options: params[:options],
      objects: params[:objects],
      edit_buttons: true,
      parent: @parent
    },
    content: @content
  ) unless params[:objects]&.include?(object.id) %>
<% end %>
